<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SimkitAssembly package="examples" version="1.0" name="ServerAssembly3" xsi:noNamespaceSchemaLocation="http://diana.nps.edu/Simkit/assembly.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <TerminalParameter type="double" value="1.7" link="interarrivalTime_2_1_1" name="interarrivalTime_2_1_1">
        <DoubleRange highValue="2.0" lowValue="1.0"/>
    </TerminalParameter>
    <TerminalParameter type="double" value="2.1" link="serviceTime_2_1_1" name="serviceTime_2_1_1">
        <DoubleRange highValue="3.0" lowValue="2.0"/>
    </TerminalParameter>
    <TerminalParameter type="double" value="2.3" link="serviceTime_2_2_1" name="serviceTime_2_2_1">
        <DoubleRange highValue="4.0" lowValue="3.0"/>
    </TerminalParameter>
    <SimEntity type="examples.ArrivalProcess" name="arrival">
        <FactoryParameter type="simkit.random.RandomVariate" factory="simkit.random.RandomVariateFactory" name="interarrivalTime">
            <TerminalParameter type="java.lang.String" value="Exponential"/>
            <MultiParameter type="java.lang.Object[]">
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter type="double" value="1.7" linkRef="interarrivalTime_2_1_1" name="interarrivalTime_2_1_1"/>
                </MultiParameter>
            </MultiParameter>
        </FactoryParameter>
        <Coordinate y="90" x="90"/>
    </SimEntity>
    <SimEntity type="examples.SimpleServer" name="server">
        <FactoryParameter type="simkit.random.RandomVariate" factory="simkit.random.RandomVariateFactory" name="serviceTime">
            <TerminalParameter type="java.lang.String" value="Gamma"/>
            <MultiParameter type="java.lang.Object[]">
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter type="double" value="2.1" linkRef="serviceTime_2_1_1" name="serviceTime_2_1_1"/>
                </MultiParameter>
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter type="double" value="2.3" linkRef="serviceTime_2_2_1" name="serviceTime_2_2_1"/>
                </MultiParameter>
            </MultiParameter>
        </FactoryParameter>
        <TerminalParameter type="int" value="2" name="totalNumberServers"/>
        <Coordinate y="90" x="280"/>
    </SimEntity>
    <PropertyChangeListener mode="replicationStat" type="simkit.stat.SimpleStatsTimeVarying" name="numInQueueStat">
        <TerminalParameter type="java.lang.String" value="numberInQueue"/>
        <Coordinate y="230" x="210"/>
    </PropertyChangeListener>
    <PropertyChangeListener mode="replicationStat" type="simkit.stat.SimpleStatsTimeVarying" name="numAvailServersStat">
        <TerminalParameter type="java.lang.String" value="numberAvailableServers"/>
        <Coordinate y="230" x="350"/>
    </PropertyChangeListener>
    <PropertyChangeListener type="simkit.util.SimplePropertyDumper" name="simplePropertyDumper">
        <Coordinate y="290" x="280"/>
    </PropertyChangeListener>
    <PropertyChangeListenerConnection source="server" listener="numInQueueStat"/>
    <PropertyChangeListenerConnection source="server" listener="numAvailServersStat"/>
    <PropertyChangeListenerConnection source="server" listener="simplePropertyDumper"/>
    <Adapter to="server" from="arrival" eventSent="Arrival" eventHeard="Arrival" name="adptr_3"/>
    <Output entity="arrival"/>
    <Output entity="server"/>
    <Schedule verbose="true" stopTime="1000.0"/>
    <Experiment timeout="5000" totalSamples="5" replicationsPerDesignPoint="2"/>
</SimkitAssembly>
