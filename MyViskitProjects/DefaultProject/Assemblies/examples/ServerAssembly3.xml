<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SimkitAssembly name="ServerAssembly3" version="1.0" package="examples" xsi:noNamespaceSchemaLocation="http://diana.nps.edu/Simkit/assembly.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <TerminalParameter name="interarrivalTime_2_1_1" link="interarrivalTime_2_1_1" value="1.7" type="double">
        <DoubleRange lowValue="1.0" highValue="2.0"/>
    </TerminalParameter>
    <TerminalParameter name="serviceTime_2_1_1" link="serviceTime_2_1_1" value="2.1" type="double">
        <DoubleRange lowValue="2.0" highValue="3.0"/>
    </TerminalParameter>
    <TerminalParameter name="serviceTime_2_2_1" link="serviceTime_2_2_1" value="2.3" type="double">
        <DoubleRange lowValue="3.0" highValue="4.0"/>
    </TerminalParameter>
    <SimEntity name="arrival" type="examples.ArrivalProcess">
        <FactoryParameter name="interarrivalTime" factory="simkit.random.RandomVariateFactory" type="simkit.random.RandomVariate">
            <TerminalParameter value="Exponential" type="java.lang.String"/>
            <MultiParameter type="java.lang.Object[]">
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter name="interarrivalTime_2_1_1" linkRef="interarrivalTime_2_1_1" value="1.7" type="double"/>
                </MultiParameter>
            </MultiParameter>
        </FactoryParameter>
        <Coordinate x="90" y="90"/>
    </SimEntity>
    <SimEntity name="server" type="examples.SimpleServer">
        <FactoryParameter name="serviceTime" factory="simkit.random.RandomVariateFactory" type="simkit.random.RandomVariate">
            <TerminalParameter value="Gamma" type="java.lang.String"/>
            <MultiParameter type="java.lang.Object[]">
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter name="serviceTime_2_1_1" linkRef="serviceTime_2_1_1" value="2.1" type="double"/>
                </MultiParameter>
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter name="serviceTime_2_2_1" linkRef="serviceTime_2_2_1" value="2.3" type="double"/>
                </MultiParameter>
            </MultiParameter>
        </FactoryParameter>
        <TerminalParameter name="totalNumberServers" value="2" type="int"/>
        <Coordinate x="280" y="90"/>
    </SimEntity>
    <PropertyChangeListener name="numInQueueStat" type="simkit.stat.SimpleStatsTimeVarying" mode="replicationStat">
        <TerminalParameter value="numberInQueue" type="java.lang.String"/>
        <Coordinate x="210" y="230"/>
    </PropertyChangeListener>
    <PropertyChangeListener name="numAvailServersStat" type="simkit.stat.SimpleStatsTimeVarying" mode="replicationStat">
        <TerminalParameter value="numberAvailableServers" type="java.lang.String"/>
        <Coordinate x="350" y="230"/>
    </PropertyChangeListener>
    <PropertyChangeListener name="simplePropertyDumper" type="simkit.util.SimplePropertyDumper">
        <Coordinate x="280" y="290"/>
    </PropertyChangeListener>
    <PropertyChangeListenerConnection listener="numInQueueStat" source="server"/>
    <PropertyChangeListenerConnection listener="numAvailServersStat" source="server"/>
    <PropertyChangeListenerConnection listener="simplePropertyDumper" source="server"/>
    <Adapter name="adptr_3" eventHeard="Arrival" eventSent="Arrival" from="arrival" to="server"/>
    <Output entity="arrival"/>
    <Output entity="server"/>
    <Schedule stopTime="1000.0" verbose="true"/>
    <Experiment replicationsPerDesignPoint="2" totalSamples="5" timeout="5000"/>
</SimkitAssembly>
