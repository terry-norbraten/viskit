<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SimkitAssembly name="ServerWithRenegesAssembly" version="1.0" package="examples" xsi:noNamespaceSchemaLocation="http://diana.nps.edu/Simkit/assembly.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <TerminalParameter name="arrivalProcess_1_1_1" link="arrivalProcess_1_1_1" value="Exponential" type="java.lang.String">
        <DoubleRange lowValue="Exponential" highValue="Exponential"/>
    </TerminalParameter>
    <TerminalParameter name="arrivalProcess_1_2_1_1_1" link="arrivalProcess_1_2_1_1_1" value="1.5" type="double">
        <DoubleRange lowValue="1.5" highValue="1.5"/>
    </TerminalParameter>
    <TerminalParameter name="serverWithReneges_1_1" link="serverWithReneges_1_1" value="2" type="int">
        <DoubleRange lowValue="2" highValue="2"/>
    </TerminalParameter>
    <TerminalParameter name="serverWithReneges_2_1_1" link="serverWithReneges_2_1_1" value="Gamma" type="java.lang.String">
        <DoubleRange lowValue="Gamma" highValue="Gamma"/>
    </TerminalParameter>
    <TerminalParameter name="serverWithReneges_2_2_1_1" link="serverWithReneges_2_2_1_1" value="new Double(2.5)" type="java.lang.Object">
        <DoubleRange lowValue="new Double(2.5)" highValue="new Double(2.5)"/>
    </TerminalParameter>
    <TerminalParameter name="serverWithReneges_2_2_2_1_1" link="serverWithReneges_2_2_2_1_1" value="1.2" type="double">
        <DoubleRange lowValue="1.2" highValue="1.2"/>
    </TerminalParameter>
    <TerminalParameter name="serverWithReneges_3_1_1" link="serverWithReneges_3_1_1" value="Uniform" type="java.lang.String">
        <DoubleRange lowValue="Uniform" highValue="Uniform"/>
    </TerminalParameter>
    <TerminalParameter name="serverWithReneges_3_2_1_1" link="serverWithReneges_3_2_1_1" value="new Integer(4)" type="java.lang.Object">
        <DoubleRange lowValue="new Integer(4)" highValue="new Integer(4)"/>
    </TerminalParameter>
    <TerminalParameter name="serverWithReneges_3_2_2_1" link="serverWithReneges_3_2_2_1" value="new Integer(6)" type="java.lang.Object">
        <DoubleRange lowValue="new Integer(6)" highValue="new Integer(6)"/>
    </TerminalParameter>
    <SimEntity name="arrivalProcess" type="examples.ArrivalProcess">
        <FactoryParameter factory="simkit.random.RandomVariateFactory" type="simkit.random.RandomVariate">
            <TerminalParameter name="arrivalProcess_1_1_1" linkRef="arrivalProcess_1_1_1" value="Exponential" type="java.lang.String"/>
            <MultiParameter type="java.lang.Object[]">
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter name="arrivalProcess_1_2_1_1_1" linkRef="arrivalProcess_1_2_1_1_1" value="1.5" type="double"/>
                </MultiParameter>
            </MultiParameter>
        </FactoryParameter>
        <Coordinate x="40" y="140"/>
    </SimEntity>
    <SimEntity name="creator" type="examples.Creator">
        <Coordinate x="190" y="140"/>
    </SimEntity>
    <SimEntity name="serverWithReneges" type="examples.ServerWithReneges">
        <TerminalParameter name="serverWithReneges_1_1" linkRef="serverWithReneges_1_1" value="2" type="int"/>
        <FactoryParameter factory="simkit.random.RandomVariateFactory" type="simkit.random.RandomVariate">
            <TerminalParameter name="serverWithReneges_2_1_1" linkRef="serverWithReneges_2_1_1" value="Gamma" type="java.lang.String"/>
            <MultiParameter type="java.lang.Object[]">
                <TerminalParameter name="serverWithReneges_2_2_1_1" linkRef="serverWithReneges_2_2_1_1" value="new Double(2.5)" type="java.lang.Object"/>
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter name="serverWithReneges_2_2_2_1_1" linkRef="serverWithReneges_2_2_2_1_1" value="1.2" type="double"/>
                </MultiParameter>
            </MultiParameter>
        </FactoryParameter>
        <FactoryParameter factory="simkit.random.RandomVariateFactory" type="simkit.random.RandomVariate">
            <TerminalParameter name="serverWithReneges_3_1_1" linkRef="serverWithReneges_3_1_1" value="Uniform" type="java.lang.String"/>
            <MultiParameter type="java.lang.Object[]">
                <TerminalParameter name="serverWithReneges_3_2_1_1" linkRef="serverWithReneges_3_2_1_1" value="new Integer(4)" type="java.lang.Object"/>
                <TerminalParameter name="serverWithReneges_3_2_2_1" linkRef="serverWithReneges_3_2_2_1" value="new Integer(6)" type="java.lang.Object"/>
            </MultiParameter>
        </FactoryParameter>
        <Coordinate x="350" y="140"/>
    </SimEntity>
    <PropertyChangeListener name="numberInQueueStat" type="simkit.stat.CollectionSizeTimeVaryingStats" mode="replicationStat">
        <TerminalParameter value="queue" type="java.lang.String"/>
        <Coordinate x="230" y="260"/>
    </PropertyChangeListener>
    <PropertyChangeListener name="numberAvailableServersStat" type="simkit.stat.SimpleStatsTally" mode="replicationStat">
        <TerminalParameter value="numberAvailableServers" type="java.lang.String"/>
        <Coordinate x="320" y="260"/>
    </PropertyChangeListener>
    <PropertyChangeListener name="numberServedStat" type="simkit.stat.SimpleStatsTimeVarying" mode="replicationStat">
        <TerminalParameter value="numberServed" type="java.lang.String"/>
        <Coordinate x="410" y="260"/>
    </PropertyChangeListener>
    <PropertyChangeListener name="numberRenegesStat" type="simkit.stat.SimpleStatsTimeVarying" mode="replicationStat">
        <TerminalParameter value="numberReneges" type="java.lang.String"/>
        <Coordinate x="510" y="260"/>
    </PropertyChangeListener>
    <PropertyChangeListener name="numberArrivalsStat" type="simkit.stat.SimpleStatsTimeVarying" mode="replicationStat">
        <TerminalParameter value="numberArrivals" type="java.lang.String"/>
        <Coordinate x="40" y="260"/>
    </PropertyChangeListener>
    <SimEventListenerConnection listener="creator" source="arrivalProcess"/>
    <PropertyChangeListenerConnection property="queue" listener="numberInQueueStat" source="serverWithReneges"/>
    <PropertyChangeListenerConnection property="numberAvailableServers" listener="numberAvailableServersStat" source="serverWithReneges"/>
    <PropertyChangeListenerConnection property="numberServed" listener="numberServedStat" source="serverWithReneges"/>
    <PropertyChangeListenerConnection property="numberReneges" listener="numberRenegesStat" source="serverWithReneges"/>
    <PropertyChangeListenerConnection property="numberArrivals" listener="numberArrivalsStat" source="arrivalProcess"/>
    <Adapter name="adptr_3" eventHeard="CustomerArrival" eventSent="Arrival" from="creator" to="serverWithReneges"/>
    <Output entity="creator"/>
    <Output entity="arrivalProcess"/>
    <Output entity="serverWithReneges"/>
    <Schedule stopTime="10000.0" verbose="true"/>
    <Experiment replicationsPerDesignPoint="1" totalSamples="1" timeout="5000"/>
</SimkitAssembly>
