<launch4jConfig>
    <!-- Header types: 0-GUI, 1-console -->
    <headerType>console</headerType>
    <outfile>stage/Viskit.exe</outfile>
    <jar>viskit-exe.jar</jar>
    <dontWrapJar>true</dontWrapJar>
    <errTitle>Viskit Install Error</errTitle>
    <chdir>.</chdir>
    <stayAlive>true</stayAlive>
    <icon>images/ViskitIcon.ico</icon>
    <jre>
      <minVersion>1.7.0</minVersion>
      <jdkPreference>jdkOnly</jdkPreference>
      <opt>-Xmx256m</opt>
      <opt>-Dswing.aatext=true</opt>
      <opt>-Djava.endorsed.dirs=lib/jaxb/endorsed</opt>
    </jre>

    <!--splash>
      <file>images/ViskitSplash2.bmp</file>
      <waitForWindow>true</waitForWindow>
      <timeout>600</timeout>
      <timeoutErr>false</timeoutErr>
    </splash-->
    <versionInfo>
      <fileVersion>0.7.0.0</fileVersion>
      <txtFileVersion>0.7.0</txtFileVersion>
      <fileDescription>Viskit launcher</fileDescription>
      <copyright>Copyright (c) 1995-2014 held by the author(s).  All rights reserved.</copyright>
      <productVersion>0.7.0.0</productVersion>
      <txtProductVersion>0.7.0</txtProductVersion>
      <productName>Viskit</productName>
      <companyName>Naval Postgraduate School</companyName>
      <internalName>Viskit</internalName>
      <originalFilename>Viskit.exe</originalFilename>
    </versionInfo>>
  </launch4jConfig>
