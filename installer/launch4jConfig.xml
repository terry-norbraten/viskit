<launch4jConfig>
    <!-- Header types: 0-GUI, 1-console -->
    <headerType>0</headerType>
    <outfile>Viskit.exe</outfile>
    <jar>stage/viskit.jar</jar>
    <dontWrapJar>false</dontWrapJar>
    <errTitle>Viskit Install Error</errTitle>
    <jarArgs></jarArgs>
    <chdir>.</chdir>
    <customProcName>true</customProcName>
    <stayAlive>true</stayAlive>
    <icon>images/ViskitIcon.ico</icon>
    <jre>
      <minVersion>1.5.0</minVersion>
      <!-- Heap sizes in MB, 0-use default -->
      <initialHeapSize>30</initialHeapSize>
      <maxHeapSize>256</maxHeapSize>
      <args>-client</args>
    </jre>
    <splash>
      <file>images/ViskitSplash2.bmp</file>
      <waitForWindow>true</waitForWindow>
      <timeout>60</timeout>
      <timeoutErr>true</timeoutErr>
    </splash>
    <versionInfo>
      <fileVersion>0.4.1.0</fileVersion>
      <txtFileVersion>0.4.1.0</txtFileVersion>
      <fileDescription>Viskit, Windows installer</fileDescription>
      <copyright>Copyright Naval Postgraduate School 2006</copyright>
      <productVersion>0.4.1.0</productVersion>
      <txtProductVersion>0.4.1.0</txtProductVersion>
      <productName>Viskit, Windows Installer</productName>
      <companyName>Naval Postgraduate School</companyName>
      <internalName>viskit.jar</internalName>
      <originalFilename>Viskit.exe</originalFilename>
    </versionInfo>
  </launch4jConfig>
