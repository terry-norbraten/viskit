<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>Viskit</name>
            <properties>
                <property name="ant.script">build.xml</property>
            </properties>
            <folders>
                <source-folder>
                    <label>src</label>
                    <type>java</type>
                    <location>src</location>
                </source-folder>
                <source-folder>
                    <label>test</label>
                    <type>java</type>
                    <location>test</location>
                </source-folder>
                <build-folder>
                    <location>../DISKIT/build/classes</location>
                </build-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <script>${ant.script}</script>
                    <target>jar</target>
                </action>
                <action name="clean">
                    <script>${ant.script}</script>
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <script>${ant.script}</script>
                    <target>doc</target>
                </action>
                <action name="run">
                    <script>${ant.script}</script>
                    <target>quick.run</target>
                </action>
                <action name="test">
                    <script>${ant.script}</script>
                    <target>test-code-format</target>
                </action>
                <action name="rebuild">
                    <script>${ant.script}</script>
                    <target>clean</target>
                    <target>jar</target>
                </action>
                <action name="run.single">
                    <script>nbproject/private/utility.xml</script>
                    <target>run-selected-file-in-Viskit</target>
                    <context>
                        <property>run.class</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="compile.single">
                    <script>nbproject/private/utility.xml</script>
                    <target>compile-selected-files-in-src</target>
                    <context>
                        <property>files</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
                <action name="debug">
                    <script>${ant.script}</script>
                    <target>debug.quick</target>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>build/classes</location>
                <script>${ant.script}</script>
                <build-target>build.viskit.no.runtime.extensions</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>../DISKIT/build/classes</location>
                <script>${ant.script}</script>
                <build-target>build.viskit.no.runtime.extensions</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src</label>
                        <location>src</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>test</label>
                        <location>test</location>
                    </source-folder>
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="run"/>
                    <ide-action name="test"/>
                    <ide-action name="debug"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/2">
            <compilation-unit>
                <package-root>src</package-root>
                <classpath mode="compile">lib/jaxb/jaxb-impl.jar:lib/jaxb/jaxb-xjc.jar:lib/actions.jar:lib/bsh-2.0b4.jar:lib/commons-beanutils-1.7.0.jar:lib/commons-codec-1.3.jar:lib/commons-collections-3.1.jar:lib/commons-digester-1.6.jar:lib/commons-logging-1.1.jar:lib/jcommon.jar:lib/jdom.jar:lib/jedit.jar:lib/jfreechart.jar:lib/jgraph.jar:lib/jhall.jar:lib/junit.jar:lib/log4j.jar:lib/looks-1.3.1.jar:lib/serializer.jar:lib/simkit.jar:lib/trang.jar:lib/trangtask.jar:lib/xalan.jar:lib/xercesImpl.jar:lib/xmlrpc-2.0.jar:lib/commons-configuration-1.5.jar:lib/commons-lang-2.4.jar</classpath>
                <built-to>build/classes</built-to>
                <built-to>../DISKIT/build/classes</built-to>
                <javadoc-built-to>doc/api</javadoc-built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>test</package-root>
                <unit-tests/>
                <classpath mode="compile">lib/simkit.jar:build/classes:lib/xmlrpc-2.0.jar</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
