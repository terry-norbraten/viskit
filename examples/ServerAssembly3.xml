<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SimkitAssembly name="ServerAssembly3" package="examples" version="1.0" xsi:noNamespaceSchemaLocation="http://diana.nps.edu/Simkit/assembly.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <TerminalParameter link="interarrivalTime_2_1_1" name="interarrivalTime_2_1_1" type="double" value="1.7">
        <DoubleRange highValue="2.0" lowValue="1.0"/>
    </TerminalParameter>
    <TerminalParameter link="serviceTime_2_1_1" name="serviceTime_2_1_1" type="double" value="2.1">
        <DoubleRange highValue="3.0" lowValue="2.0"/>
    </TerminalParameter>
    <TerminalParameter link="serviceTime_2_2_1" name="serviceTime_2_2_1" type="double" value="2.3">
        <DoubleRange highValue="4.0" lowValue="3.0"/>
    </TerminalParameter>
    <SimEntity name="arrival" type="examples.ArrivalProcess">
        <FactoryParameter factory="simkit.random.RandomVariateFactory" name="interarrivalTime" type="simkit.random.RandomVariate">
            <TerminalParameter type="java.lang.String" value="Exponential"/>
            <MultiParameter type="java.lang.Object[]">
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter linkRef="interarrivalTime_2_1_1" name="interarrivalTime_2_1_1" type="double" value="1.7"/>
                </MultiParameter>
            </MultiParameter>
        </FactoryParameter>
        <Coordinate x="90" y="90"/>
    </SimEntity>
    <SimEntity name="server" type="examples.SimpleServer">
        <FactoryParameter factory="simkit.random.RandomVariateFactory" name="serviceTime" type="simkit.random.RandomVariate">
            <TerminalParameter type="java.lang.String" value="Gamma"/>
            <MultiParameter type="java.lang.Object[]">
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter linkRef="serviceTime_2_1_1" name="serviceTime_2_1_1" type="double" value="2.1"/>
                </MultiParameter>
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter linkRef="serviceTime_2_2_1" name="serviceTime_2_2_1" type="double" value="2.3"/>
                </MultiParameter>
            </MultiParameter>
        </FactoryParameter>
        <TerminalParameter name="totalNumberServers" type="int" value="2"/>
        <Coordinate x="280" y="90"/>
    </SimEntity>
    <PropertyChangeListener mode="replicationStats" name="numInQueueStat" type="simkit.stat.SimpleStatsTimeVarying">
        <TerminalParameter type="java.lang.String" value="numberInQueue"/>
        <Coordinate x="210" y="230"/>
    </PropertyChangeListener>
    <PropertyChangeListener mode="replicationStats" name="numAvailServersStat" type="simkit.stat.SimpleStatsTimeVarying">
        <TerminalParameter type="java.lang.String" value="numberAvailableServers"/>
        <Coordinate x="350" y="230"/>
    </PropertyChangeListener>
    <PropertyChangeListener name="simplePropertyDumper" type="simkit.util.SimplePropertyDumper">
        <Coordinate x="280" y="290"/>
    </PropertyChangeListener>
    <PropertyChangeListenerConnection listener="numInQueueStat" source="server"/>
    <PropertyChangeListenerConnection listener="numAvailServersStat" source="server"/>
    <PropertyChangeListenerConnection listener="simplePropertyDumper" source="server"/>
    <Adapter eventHeard="Arrival" eventSent="Arrival" from="arrival" name="adptr_3" to="server"/>
    <Output entity="arrival"/>
    <Output entity="server"/>
    <Schedule stopTime="1000.0" verbose="true"/>
    <Experiment replicationsPerDesignPoint="2" timeout="5000" totalSamples="5"/>
</SimkitAssembly>

