<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SimkitAssembly version="1.0" package="examples" name="ServerAssembly3" xsi:noNamespaceSchemaLocation="http://diana.gl.nps.navy.mil/Simkit/assembly.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <SimEntity name="arrival" type="examples.ArrivalProcess">
        <FactoryParameter type="simkit.random.RandomVariate" factory="simkit.random.RandomVariateFactory">
            <TerminalParameter value="Exponential" type="java.lang.String"/>
            <MultiParameter type="java.lang.Object[]">
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter value="1.7" type="double"/>
                </MultiParameter>
            </MultiParameter>
            <TerminalParameter value="CongruentialSeeds.SEED[0]" type="long"/>
        </FactoryParameter>
        <Coordinate y="98" x="86"/>
    </SimEntity>
    <SimEntity name="server" type="examples.SimpleServer">
        <FactoryParameter type="simkit.random.RandomVariate" factory="simkit.random.RandomVariateFactory">
            <TerminalParameter value="Gamma" type="java.lang.String"/>
            <MultiParameter type="java.lang.Object[]">
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter value="2.1" type="double"/>
                </MultiParameter>
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter value="2.3" type="double"/>
                </MultiParameter>
            </MultiParameter>
            <TerminalParameter value="CongruentialSeeds.SEED[1]" type="long"/>
        </FactoryParameter>
        <TerminalParameter value="2" type="int"/>
        <Coordinate y="98" x="286"/>
    </SimEntity>
    <PropertyChangeListener name="numInQueueStat" type="simkit.stat.SimpleStatsTimeVarying">
        <TerminalParameter value="numberInQueue" type="java.lang.String"/>
        <Coordinate y="236" x="206"/>
    </PropertyChangeListener>
    <PropertyChangeListener name="numAvailServersStat" type="simkit.stat.SimpleStatsTimeVarying">
        <TerminalParameter value="numberAvailableServers" type="java.lang.String"/>
        <Coordinate y="238" x="356"/>
    </PropertyChangeListener>
    <PropertyChangeListenerConnection listener="numInQueueStat" source="server"/>
    <PropertyChangeListenerConnection listener="numAvailServersStat" source="server"/>
    <Adapter eventSent="Arrival" from="arrival" name="adptr_3" eventHeard="Arrival" to="server"/>
    <Output entity="arrival"/>
    <Output entity="server"/>
    <Schedule verbose="true" stopTime="1000.0"/>
</SimkitAssembly>