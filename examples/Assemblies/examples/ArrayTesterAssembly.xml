<SimkitAssembly name="ArrayTestAssembly" package="examples" version="1.0" xsi:noNamespaceSchemaLocation="http://diana.nps.edu/Simkit/assembly.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <SimEntity description="" name="ArrayTester_3" type="examples.ArrayTester">
        <MultiParameter type="int[]">
            <TerminalParameter name="" type="int" value="17"/>
