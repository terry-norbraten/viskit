<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SimkitAssembly name="ServerWithRenegesAssembly" package="examples" version="1.0" xsi:noNamespaceSchemaLocation="http://diana.nps.edu/Simkit/assembly.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <TerminalParameter link="arrivalProcess_1_1_1" name="arrivalProcess_1_1_1" type="java.lang.String" value="Exponential">
        <DoubleRange highValue="Exponential" lowValue="Exponential"/>
    </TerminalParameter>
    <TerminalParameter link="arrivalProcess_1_2_1_1_1" name="arrivalProcess_1_2_1_1_1" type="double" value="1.5">
        <DoubleRange highValue="1.5" lowValue="1.5"/>
    </TerminalParameter>
    <TerminalParameter link="serverWithReneges_1_1" name="serverWithReneges_1_1" type="int" value="2">
        <DoubleRange highValue="2" lowValue="2"/>
    </TerminalParameter>
    <TerminalParameter link="serverWithReneges_2_1_1" name="serverWithReneges_2_1_1" type="java.lang.String" value="Gamma">
        <DoubleRange highValue="Gamma" lowValue="Gamma"/>
    </TerminalParameter>
    <TerminalParameter link="serverWithReneges_2_2_1_1" name="serverWithReneges_2_2_1_1" type="java.lang.Object" value="new Double(2.5)">
        <DoubleRange highValue="new Double(2.5)" lowValue="new Double(2.5)"/>
    </TerminalParameter>
    <TerminalParameter link="serverWithReneges_2_2_2_1_1" name="serverWithReneges_2_2_2_1_1" type="double" value="1.2">
        <DoubleRange highValue="1.2" lowValue="1.2"/>
    </TerminalParameter>
    <TerminalParameter link="serverWithReneges_3_1_1" name="serverWithReneges_3_1_1" type="java.lang.String" value="Uniform">
        <DoubleRange highValue="Uniform" lowValue="Uniform"/>
    </TerminalParameter>
    <TerminalParameter link="serverWithReneges_3_2_1_1" name="serverWithReneges_3_2_1_1" type="java.lang.Object" value="new Integer(4)">
        <DoubleRange highValue="new Integer(4)" lowValue="new Integer(4)"/>
    </TerminalParameter>
    <TerminalParameter link="serverWithReneges_3_2_2_1" name="serverWithReneges_3_2_2_1" type="java.lang.Object" value="new Integer(6)">
        <DoubleRange highValue="new Integer(6)" lowValue="new Integer(6)"/>
    </TerminalParameter>
    <SimEntity name="arrivalProcess" type="examples.ArrivalProcess">
        <FactoryParameter factory="simkit.random.RandomVariateFactory" type="simkit.random.RandomVariate">
            <TerminalParameter linkRef="arrivalProcess_1_1_1" name="arrivalProcess_1_1_1" type="java.lang.String" value="Exponential"/>
            <MultiParameter type="java.lang.Object[]">
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter linkRef="arrivalProcess_1_2_1_1_1" name="arrivalProcess_1_2_1_1_1" type="double" value="1.5"/>
                </MultiParameter>
            </MultiParameter>
        </FactoryParameter>
        <Coordinate x="40" y="140"/>
    </SimEntity>
    <SimEntity name="creator" type="examples.Creator">
        <Coordinate x="190" y="140"/>
    </SimEntity>
    <SimEntity name="serverWithReneges" type="examples.ServerWithReneges">
        <TerminalParameter linkRef="serverWithReneges_1_1" name="serverWithReneges_1_1" type="int" value="2"/>
        <FactoryParameter factory="simkit.random.RandomVariateFactory" type="simkit.random.RandomVariate">
            <TerminalParameter linkRef="serverWithReneges_2_1_1" name="serverWithReneges_2_1_1" type="java.lang.String" value="Gamma"/>
            <MultiParameter type="java.lang.Object[]">
                <TerminalParameter linkRef="serverWithReneges_2_2_1_1" name="serverWithReneges_2_2_1_1" type="java.lang.Object" value="new Double(2.5)"/>
                <MultiParameter type="java.lang.Double">
                    <TerminalParameter linkRef="serverWithReneges_2_2_2_1_1" name="serverWithReneges_2_2_2_1_1" type="double" value="1.2"/>
                </MultiParameter>
            </MultiParameter>
        </FactoryParameter>
        <FactoryParameter factory="simkit.random.RandomVariateFactory" type="simkit.random.RandomVariate">
            <TerminalParameter linkRef="serverWithReneges_3_1_1" name="serverWithReneges_3_1_1" type="java.lang.String" value="Uniform"/>
            <MultiParameter type="java.lang.Object[]">
                <TerminalParameter linkRef="serverWithReneges_3_2_1_1" name="serverWithReneges_3_2_1_1" type="java.lang.Object" value="new Integer(4)"/>
                <TerminalParameter linkRef="serverWithReneges_3_2_2_1" name="serverWithReneges_3_2_2_1" type="java.lang.Object" value="new Integer(6)"/>
            </MultiParameter>
        </FactoryParameter>
        <Coordinate x="350" y="140"/>
    </SimEntity>
    <PropertyChangeListener mode="replicationStats" name="numberInQueueStat" type="simkit.stat.CollectionSizeTimeVaryingStats">
        <TerminalParameter type="java.lang.String" value="queue"/>
        <Coordinate x="230" y="260"/>
    </PropertyChangeListener>
    <PropertyChangeListener mode="replicationStats" name="numberAvailableServersStat" type="simkit.stat.SimpleStatsTally">
        <TerminalParameter type="java.lang.String" value="numberAvailableServers"/>
        <Coordinate x="320" y="260"/>
    </PropertyChangeListener>
    <PropertyChangeListener mode="replicationStats" name="numberServedStat" type="simkit.stat.SimpleStatsTimeVarying">
        <TerminalParameter type="java.lang.String" value="numberServed"/>
        <Coordinate x="410" y="260"/>
    </PropertyChangeListener>
    <PropertyChangeListener mode="replicationStats" name="numberRenegesStat" type="simkit.stat.SimpleStatsTimeVarying">
        <TerminalParameter type="java.lang.String" value="numberReneges"/>
        <Coordinate x="510" y="260"/>
    </PropertyChangeListener>
    <PropertyChangeListener mode="replicationStats" name="numberArrivalsStat" type="simkit.stat.SimpleStatsTimeVarying">
        <TerminalParameter type="java.lang.String" value="numberArrivals"/>
        <Coordinate x="40" y="260"/>
    </PropertyChangeListener>
    <SimEventListenerConnection listener="creator" source="arrivalProcess"/>
    <PropertyChangeListenerConnection listener="numberInQueueStat" property="queue" source="serverWithReneges"/>
    <PropertyChangeListenerConnection listener="numberAvailableServersStat" property="numberAvailableServers" source="serverWithReneges"/>
    <PropertyChangeListenerConnection listener="numberServedStat" property="numberServed" source="serverWithReneges"/>
    <PropertyChangeListenerConnection listener="numberRenegesStat" property="numberReneges" source="serverWithReneges"/>
    <PropertyChangeListenerConnection listener="numberArrivalsStat" property="numberArrivals" source="arrivalProcess"/>
    <Adapter eventHeard="CustomerArrival" eventSent="Arrival" from="creator" name="adptr_3" to="serverWithReneges"/>
    <Output entity="creator"/>
    <Output entity="arrivalProcess"/>
    <Output entity="serverWithReneges"/>
    <Schedule stopTime="10000.0" verbose="true"/>
    <Experiment replicationsPerDesignPoint="1" timeout="5000" totalSamples="1"/>
</SimkitAssembly>
