<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SimkitAssembly name="BarrierSearchAssembly" package="seadiver" version="$Id$" xsi:noNamespaceSchemaLocation="http://diana.gl.nps.navy.mil/Simkit/assembly.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <SimEntity description="List of identical seadiver robots that perform a cooperative search. Units are m/s." name="SeaDivers" type="seadiver.SeaDiver">
        <TerminalParameter name="maximumSpeed" type="double" value="1.53"/>
        <TerminalParameter name="firstMoverID" type="int" value="100"/>
        <TerminalParameter name="numberDivers" type="int" value="100"/>
        <TerminalParameter name="sensorRange" type="double" value="500"/>
        <MultiParameter type="diskit.random.RandomVariateInstantiator">
            <MultiParameter type="diskit.random.AR1Variate"/>
            <MultiParameter type="diskit.random.BernoulliVariate"/>
            <MultiParameter type="diskit.random.BetaVariate"/>
            <MultiParameter type="diskit.random.BinomialVariate"/>
            <MultiParameter type="diskit.random.DiscreteUniformVariate"/>
            <MultiParameter type="diskit.random.ExponentialVariate">
                <TerminalParameter name="mean (-Infinity -&gt; +Infinity)" type="double" value="300"/>
            </MultiParameter>
            <MultiParameter type="diskit.random.GammaVariate"/>
            <MultiParameter type="diskit.random.InverseGaussianVariate"/>
            <MultiParameter type="diskit.random.NormalVariate"/>
            <MultiParameter type="diskit.random.PoissonVariate"/>
            <MultiParameter type="diskit.random.UniformVariate"/>
        </MultiParameter>
        <TerminalParameter name="inputFileTemplate" type="java.lang.String" value="G:/ThesisCode/MissionFiles/SeaDiverTemplate.xml"/>
        <TerminalParameter name="outputFileName" type="java.lang.String" value="G:/ThesisCode/MissionFiles/BarrierSearch/Diver.xml"/>
        <TerminalParameter name="operatingDepth" type="double" value="50"/>
        <Coordinate x="70" y="170"/>
    </SimEntity>
    <SimEntity description="Defines the overall search area for operation by targets and subdivided search by seadivers. Units are m/s." name="ZoneMap" type="seadiver.ZoneMap">
        <MultiParameter type="diskit.X3DCoordinate">
            <TerminalParameter name="X" type="double" value="0"/>
            <TerminalParameter name="Y (+Y is up)" type="double" value="0"/>
            <TerminalParameter name="Z" type="double" value="0"/>
        </MultiParameter>
        <TerminalParameter name="length" type="double" value="200000"/>
        <TerminalParameter name="width" type="double" value="200000"/>
        <TerminalParameter name="height" type="double" value="100"/>
        <Coordinate x="240" y="170"/>
    </SimEntity>
    <SimEntity description="Controls the movement and detection of all targets and seadivers, along with providing DIS output." name="ScenarioManager" type="diskit.ScenarioManager">
        <TerminalParameter name="speedScale" type="double" value="1.0"/>
        <TerminalParameter name="clearOnReset" type="boolean" value="true"/>
        <TerminalParameter name="mulicastIPAddress" type="java.lang.String" value="239.1.2.3"/>
        <TerminalParameter name="port" type="int" value="60000"/>
        <TerminalParameter name="siteID" type="int" value="0"/>
        <TerminalParameter name="appID" type="int" value="1"/>
        <Coordinate x="70" y="310"/>
    </SimEntity>
    <SimEntity description="List of identical target ships/submarines to be detected.  Units are m/s." name="Targets" type="seadiver.Target">
        <TerminalParameter name="speed" type="double" value="6"/>
        <TerminalParameter name="numberOfTargets" type="int" value="10"/>
        <TerminalParameter name="numberOfWaypoints" type="int" value="1"/>
        <MultiParameter type="diskit.random.RandomVariateInstantiator">
            <MultiParameter type="diskit.random.AR1Variate"/>
            <MultiParameter type="diskit.random.BernoulliVariate"/>
            <MultiParameter type="diskit.random.BetaVariate"/>
            <MultiParameter type="diskit.random.BinomialVariate"/>
            <MultiParameter type="diskit.random.DiscreteUniformVariate"/>
            <MultiParameter type="diskit.random.ExponentialVariate"/>
            <MultiParameter type="diskit.random.GammaVariate"/>
            <MultiParameter type="diskit.random.InverseGaussianVariate"/>
            <MultiParameter type="diskit.random.NormalVariate">
                <TerminalParameter name="Mean (-Infinity -&gt; +Infinity)" type="double" value=".5"/>
                <TerminalParameter name="Standard Deviation (0.0 -&gt; +Infinity)" type="double" value=".25"/>
            </MultiParameter>
            <MultiParameter type="diskit.random.PoissonVariate"/>
            <MultiParameter type="diskit.random.UniformVariate"/>
        </MultiParameter>
        <TerminalParameter name="inputFileTemplate" type="java.lang.String" value="G:/ThesisCode/MissionFiles/SeadiverTemplate.xml"/>
        <TerminalParameter name="outputFileName" type="java.lang.String" value="G:/ThesisCode/MissionFiles/BarrierSearch/Target.xml"/>
        <TerminalParameter name="operatingDepth" type="double" value="50"/>
        <TerminalParameter name="sensorRange" type="double" value="100"/>
        <Coordinate x="240" y="310"/>
    </SimEntity>
    <PropertyChangeListener description="" mode="replicationStats" name="TargetDetections" type="simkit.stat.SimpleStatsTally">
        <TerminalParameter name="p[0] : " type="java.lang.String" value="targetDetections"/>
        <Coordinate x="70" y="60"/>
    </PropertyChangeListener>
    <PropertyChangeListener description="" mode="replicationStats" name="SeadiverDetections" type="simkit.stat.SimpleStatsTally">
        <TerminalParameter name="p[0] : " type="java.lang.String" value="numberOfSeadiverDetections"/>
        <Coordinate x="360" y="310"/>
    </PropertyChangeListener>
    <SimEventListenerConnection listener="ScenarioManager" source="SeaDivers"/>
    <SimEventListenerConnection description="Connects the CreateMap events in each event graph." listener="ZoneMap" source="SeaDivers"/>
    <SimEventListenerConnection listener="SeaDivers" source="ZoneMap"/>
    <SimEventListenerConnection listener="Targets" source="ZoneMap"/>
    <SimEventListenerConnection listener="ZoneMap" source="Targets"/>
    <SimEventListenerConnection listener="ScenarioManager" source="Targets"/>
    <PropertyChangeListenerConnection description="" listener="TargetDetections" property="targetDetections" source="SeaDivers"/>
    <PropertyChangeListenerConnection description="" listener="SeadiverDetections" property="numberOfSeadiverDetections" source="Targets"/>
    <Schedule stopTime="100." verbose="false"/>
    <Experiment replicationsPerDesignPoint="1" timeout="5000" totalSamples="1"/>
</SimkitAssembly>

