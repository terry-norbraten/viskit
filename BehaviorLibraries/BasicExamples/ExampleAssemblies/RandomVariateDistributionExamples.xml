            <TerminalParameter name="upperBounds" type="double" value="30"/>
        </MultiParameter>
        <Coordinate x="160" y="130"/>
    </SimEntity>
    <Schedule stopTime="100." verbose="false"/>
    <Experiment replicationsPerDesignPoint="1" timeout="5000" totalSamples="1"/>
</SimkitAssembly>

